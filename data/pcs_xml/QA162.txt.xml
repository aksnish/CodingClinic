<problem value="bleeding" begin="441" end="449" status="0" certainty="0" cui="C0019080" tui="T046" rank="791" code="NO-CODE" sourceType="ICPC2P"/>
</RelationSet>
</Sentence>
<Sentence sentNo="5" begin="451" end="613" sentText="The root operation “Control” is only available in the general anatomic regions, and in this case “genitourinary tract” is the appropriate general anatomic region.">
</Sentence>
